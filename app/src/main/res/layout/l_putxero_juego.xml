<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout11"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".f_putxero_juego">

    tools:viewBindingIgnore="true"

    <TextView
        android:id="@+id/txt_putxero_titulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="@string/putxero_juego"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imgputxero_juego_logo"
        android:layout_width="182dp"
        android:layout_height="121dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_putxero_titulo"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/img_putxero_tren"
        android:layout_width="233dp"
        android:layout_height="135dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="4dp"
        android:src="@drawable/tren"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgputxero_juego_logo" />

    <ImageView
        android:id="@+id/img_putxero_putxero"
        android:layout_width="85dp"
        android:layout_height="129dp"
        android:layout_marginTop="56dp"
        android:src="@drawable/puchero"
        app:layout_constraintEnd_toStartOf="@+id/img_putxero_tren"
        app:layout_constraintHorizontal_bias="0.303"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgputxero_juego_logo"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/img_pimiento"
        android:layout_width="80dp"
        android:layout_height="71dp"
        android:layout_marginStart="305dp"
        android:layout_marginTop="200dp"
        android:src="@drawable/pimiento"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_putxero_tren" />

    <ImageView
        android:id="@+id/img_alubias"
        android:layout_width="83dp"
        android:layout_height="71dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="115dp"
        android:src="@drawable/alubias"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_putxero_putxero" />

    <ImageView
        android:id="@+id/img_morcilla"
        android:layout_width="85dp"
        android:layout_height="70dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="192dp"
        android:src="@drawable/morcilla"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_putxero_putxero" />

    <ImageView
        android:id="@+id/img_aceite"
        android:layout_width="83dp"
        android:layout_height="70dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="120dp"
        android:src="@drawable/aceite"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_putxero_putxero" />

    <ImageView
        android:id="@+id/img_arroz"
        android:layout_width="81dp"
        android:layout_height="70dp"
        android:layout_marginStart="205dp"
        android:layout_marginTop="125dp"
        android:src="@drawable/arroz"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_putxero_tren" />

    <ImageView
        android:id="@+id/img_cebolla"
        android:layout_width="82dp"
        android:layout_height="70dp"
        android:layout_marginStart="305dp"
        android:layout_marginTop="125dp"
        android:src="@drawable/cebolla"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_putxero_tren" />

    <ImageView
        android:id="@+id/img_chorizo"
        android:layout_width="81dp"
        android:layout_height="69dp"
        android:layout_marginStart="205dp"
        android:layout_marginTop="200dp"
        android:src="@drawable/chorizo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_putxero_tren" />

    <ImageView
        android:id="@+id/img_costilla"
        android:layout_width="80dp"
        android:layout_height="69dp"
        android:layout_marginStart="300dp"
        android:layout_marginTop="45dp"
        android:src="@drawable/costilla"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_putxero_tren" />

    <ImageView
        android:id="@+id/img_lechuga"
        android:layout_width="81dp"
        android:layout_height="66dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="196dp"
        android:src="@drawable/lechuga"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_putxero_putxero" />

    <ImageView
        android:id="@+id/img_pepinillo"
        android:layout_width="68dp"
        android:layout_height="70dp"
        android:layout_marginStart="230dp"
        android:layout_marginTop="44dp"
        android:src="@drawable/pepinillo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_putxero_tren" />

    <ImageView
        android:id="@+id/img_sal"
        android:layout_width="54dp"
        android:layout_height="69dp"
        android:layout_marginStart="175dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/sal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_putxero_putxero" />

    <ImageView
        android:id="@+id/img_tomate"
        android:layout_width="81dp"
        android:layout_height="69dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/tomate"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_putxero_putxero" />

    <ImageView
        android:id="@+id/img_zanahoria"
        android:layout_width="81dp"
        android:layout_height="71dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="38dp"
        android:src="@drawable/zanahoria"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_putxero_putxero" />

    <Button
        android:id="@+id/btn_putxerojuego"
        style="@style/botones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/txt_finalizar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.949"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />

</androidx.constraintlayout.widget.ConstraintLayout>