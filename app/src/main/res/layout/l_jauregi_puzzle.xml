<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout7"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".f_jauregi_puzzle">

    tools:viewBindingIgnore="true"


    <TextView
        android:id="@+id/txtpuente_puzzle_titulo"
        style="@style/titulo2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/txtjauregi_puzzle_titulo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.524"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtpuente_puzzle_explicacion"
        style="@style/juegos_explicacion"
        android:layout_width="342dp"
        android:layout_height="87dp"
        android:layout_marginTop="16dp"
        android:text="@string/txtjauregi_puzzle_explicacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgjauregi_juego_logo" />

    <ImageButton
        android:id="@+id/imgjauregi_juego_logo"
        style="@style/medidas_dantzaris"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtpuente_puzzle_titulo"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/img_jauregi_foto"
        android:layout_width="342dp"
        android:layout_height="252dp"
        android:layout_marginTop="32dp"
        android:alpha="600"
        android:src="@drawable/palaciohorcasitasbalmaseda"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.362"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtpuente_puzzle_explicacion" />


    <ImageView
        android:id="@+id/posicion_puertaprincipal"
        android:layout_width="41dp"
        android:layout_height="76dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="148dp"
        android:src="@drawable/pieza_puertaprincipal_posicion"
        app:layout_constraintStart_toStartOf="@+id/img_jauregi_foto"
        app:layout_constraintTop_toTopOf="@+id/img_jauregi_foto" />

    <ImageView
        android:id="@+id/posicion_columna"
        android:layout_width="34dp"
        android:layout_height="96dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="128dp"

        android:src="@drawable/pieza_columna_posicion"
        app:layout_constraintStart_toStartOf="@+id/img_jauregi_foto"
        app:layout_constraintTop_toTopOf="@+id/img_jauregi_foto" />

    <ImageView
        android:id="@+id/posicion_puertaventana"
        android:layout_width="66dp"
        android:layout_height="49dp"
        android:layout_marginStart="232dp"
        android:layout_marginTop="108dp"

        android:src="@drawable/pieza_puertaventana_posicion"
        app:layout_constraintStart_toStartOf="@+id/img_jauregi_foto"
        app:layout_constraintTop_toTopOf="@+id/img_jauregi_foto" />

    <ImageView
        android:id="@+id/posicion_balcon"
        android:layout_width="49dp"
        android:layout_height="57dp"
        android:layout_marginStart="164dp"
        android:layout_marginTop="88dp"
        android:src="@drawable/pieza_balcon_posicion"
        app:layout_constraintStart_toStartOf="@+id/img_jauregi_foto"
        app:layout_constraintTop_toTopOf="@+id/img_jauregi_foto" />

    <ImageView
        android:id="@+id/posicion_escudo"
        android:layout_width="22dp"
        android:layout_height="28dp"
        android:layout_marginStart="200dp"
        android:layout_marginTop="36dp"
        android:src="@drawable/pieza_escudo_posicion"
        app:layout_constraintStart_toStartOf="@+id/img_jauregi_foto"
        app:layout_constraintTop_toTopOf="@+id/img_jauregi_foto" />

    <ImageView
        android:id="@+id/posicion_ventana"
        android:layout_width="46dp"
        android:layout_height="43dp"
        android:layout_marginStart="260dp"
        android:layout_marginTop="156dp"
        android:src="@drawable/pieza_ventana_posicion"
        app:layout_constraintStart_toStartOf="@+id/img_jauregi_foto"
        app:layout_constraintTop_toTopOf="@+id/img_jauregi_foto" />

    <ImageView
        android:id="@+id/img_pieza_columna"
        android:layout_width="25dp"
        android:layout_height="54dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="584dp"
        android:src="@drawable/pieza_columna"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_pieza_balcon"
        android:layout_width="58dp"
        android:layout_height="58dp"
        android:layout_marginStart="308dp"
        android:layout_marginTop="576dp"
        android:src="@drawable/pieza_balcon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_pieza_ventana"
        android:layout_width="55dp"
        android:layout_height="53dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="580dp"
        android:src="@drawable/pieza_ventana"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_pieza_puertaprincipal"
        android:layout_width="46dp"
        android:layout_height="53dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="584dp"
        android:src="@drawable/pieza_puertaprincipal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_pieza_escudo"
        android:layout_width="57dp"
        android:layout_height="55dp"
        android:layout_marginStart="184dp"
        android:layout_marginTop="580dp"
        android:src="@drawable/pieza_escudo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_pieza_puertaventana"
        android:layout_width="47dp"
        android:layout_height="48dp"
        android:layout_marginStart="248dp"
        android:layout_marginTop="584dp"
        android:src="@drawable/pieza_puertaventana"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>